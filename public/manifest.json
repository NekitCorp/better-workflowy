{
    "name": "Better WorkFlowy",
    "description": "Enhance WorkFlowy",
    "version": "1.2",
    "manifest_version": 2,
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.workflowy.com/*", "*://workflowy.com/*"],
            "js": ["build/content_scripts.js"]
        }
    ],
    "permissions": ["storage"]
}
